<script>
    $(document).ready(function () {
        showPage5('@ViewBag.Page5');
        generateQR();
        GenerateBarCode();
        window.print();
    });

    //QR Component = qrcodejs
    //https://github.com/davidshimjs/qrcodejs

    //Init QR CODE
    var qrcode = new QRCode(document.getElementById("qrcode"),
        {
            text: "",
            width: 80,
            height: 80,
            colorDark: "#000000",
            colorLight: "#ffffff",
            correctLevel: QRCode.CorrectLevel.H
        });

    function generateQR() {
        qrcode.clear();
        qrcode.makeCode('http://www.siamsmile.co.th/');
    };

    function GenerateBarCode() {
        JsBarcode("#barcode", $('#hidden_hciCode').val(), { format: 'CODE128' });
    }

    const showPage5 = (p) => {
        if (p === 'True') {
            $('#page5').show();
        } else {
            $('#page5').hide();
        }
    }
</script>