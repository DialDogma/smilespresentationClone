    #form {
        width: 100%;
    }

    table {
        width: 100%;
        border-collapse: collapse;
    }

    .table-p2, .td-p2 {
        border: 1px solid;
        padding: 0;
        margin: 0;
    }

    th {
        font: large arial, tahoma, sans-serif;
        font-weight: bold;
        padding: 10px;
    }

    #table-p2-main {
        width: 1022px;
        margin: auto;
    }

    #row2-p2 {
