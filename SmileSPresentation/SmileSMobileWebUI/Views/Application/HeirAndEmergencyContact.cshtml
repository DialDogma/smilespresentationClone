@using Microsoft.Ajax.Utilities
@{
    ViewBag.Title = "HeirAndEmergencyContact";
    Layout = "~/Views/Shared/_MasterApplication.cshtml";
}
<style>
    .bgButton {
        text-align: center;
        background-color: #337ab7;
        cursor: pointer;
        /*height: 69px;*/
        position: fixed;
        bottom: 0;
        left: 0;
        /*margin-top: 50px;*/
    }

    .btnNext {
        /*padding: 10px;*/
        font-size: 25px;
        color: #fff;
        background-color: #337ab7;
        border-color: transparent;
        position: fixed;
