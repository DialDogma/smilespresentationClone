        function CheckIdentityCard(cardId) {
            if (cardId.length != 13) return false;
            var sum = 0;
            for (var i = 0; i < 12; i++) {
                sum += parseFloat(cardId.charAt(i)) * (13 - i);
            }
            if ((11 - (sum % 11)) % 10 != parseFloat(cardId.charAt(12))) {
                return false;
            }

            return true;
        }

