@{
    /**/

    ViewBag.Title = "รายละเอียด";
    Layout = "~/Views/Shared/_LayoutForTopPHIndex.cshtml";
}
<style>
    strong {
        color: red;
    }

    label.title {
        font-family: Sarabun;
        color: #3F8FBD
    }

    label.fonts-box {
        font-family: Sarabun;
        font-size: 14px;
        font-weight: lighter;
    }

    h4.font-custom {
        font-family: Sarabun;
    }

    h5.font-custom {
        font-family: Sarabun;
    }
</style>
<section class="content">
    <div class="nav-tabs-custom">
        <ul class="nav nav-tabs">
            <li class="active"><a href="#tab_1" data-toggle="tab" aria-expanded="false">บันทึกผลการทำงาน</a></li>
            <li class=""><a href="#tab_2" data-toggle="tab" aria-expanded="false">ผลการคัดกรองสาขา</a></li>
            <li class=""><a href="#tab_3" data-toggle="tab" aria-expanded="true">เอกสาร</a></li>
            <li class=""><a href="#tab_4" data-toggle="tab" aria-expanded="true">ประวัติคิวงาน</a></li>
        </ul>
        @if (ViewBag.QueueFullDetail != null)
        {
            <div class="tab-content">
                @* tab_1 *@
                <div class="tab-pane active view-detail" id="tab_1">
                    <div class="row ">
                        <div class="col-sm-12 ">
                            <div class="card" style="background-color: #E0F7FF; border-radius: 8px 8px; margin-left: 10px; margin-right: 10px ">
                                <div class="box-header" style="color: #3F8FBD; border-bottom: 1px solid #D2D6DE">
                                    <i class="fa fa-user"></i>
                                    <h4 class="box-title font-custom">ข้อมูลผู้เอาประกัน </h4>
                                </div>
                                <div class="box-body" style="align-items: center;">
                                    <div class="row">
                                        <h5 class="font-custom col-md-3"><b>App ID</b> : <a id="openSSS" style="text-decoration: underline; color: #2833FF " onclick="OpenSSS(@ViewBag.QueueFullDetail.ApplicationCode)">@ViewBag.QueueFullDetail.ApplicationCode</a></h5>
                                        <h5 class="font-custom col-md-3">
                                            <b>วันเริ่มคุ้มครอง</b>  : @(String.Format("{0:dd MMM yyyy}", ViewBag.QueueFullDetail.AppStartCoverDate))
                                        </h5>
                                        <h5 class="font-custom col-md-3"><b>แผนประกัน</b> : @ViewBag.QueueFullDetail.Product</h5>
                                        <h5 class="font-custom col-md-3"><b>เบี้ย</b> : @ViewBag.QueueFullDetail.Premium บาท</h5>
                                    </div>
                                    <div class="row">
                                        <h5 class="font-custom col-md-3 "><b>ชื่อ</b> : @(String.Format("{0}", ViewBag.QueueFullDetail.InsuredName))</h5>
                                        <h5 class="font-custom col-md-3"><b>วันเกิด</b> : @(String.Format("{0:dd MMM yyyy}", ViewBag.QueueFullDetail.InsuredBirthDate))</h5>
                                        <h5 class="font-custom col-md-3"><b>อายุวันสมัคร</b> : @ViewBag.QueueFullDetail.AgesAtRegister</h5>
                                        <h5 class="font-custom col-md-3"><b>อายุปัจจุบัน</b> :  @ViewBag.QueueFullDetail.AgesCurrent</h5>
                                    </div>
                                    <div class="row">
                                        <h5 class="font-custom col-md-3"><b>เบอร์โทร</b> : @ViewBag.QueueFullDetail.InsuredPhone</h5>
                                        <h5 class="font-custom col-md-3">
                                            <b>สถานะ</b> : @ViewBag.QueueFullDetail.AppStatus
                                        </h5>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-md-12" style="text-align:right">
                                            <a href="#" id="btnPolicyOther" data-toggle="modal" data-target="#modal-warning-policy" style="text-decoration: underline; color: #2833FF; display: none "></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12" style="margin-top:15px;margin-bottom :15px">
                            <div class="card" style="background-color: #E0F7FF; border-radius: 8px 8px; margin-left: 10px; margin-right: 10px">

                                <div class="box-header" style="color: #3F8FBD; border-bottom: 1px solid #D2D6DE">
                                    <i class="fa fa-user"></i>
                                    <h4 class="font-custom box-title">ข้อมูลผู้ชำระเบี้ย </h4>
                                </div>
                                <div class="box-body " style="align-items: center;">
                                    <div class="row">
                                        <h5 class="font-custom col-md-4 font-custom"><b>ชื่อผู้ชำระเบี้ย</b> : @(ViewBag.QueueFullDetail.PayerName != null ? String.Format("{0}",  ViewBag.QueueFullDetail.PayerName) : "-")</h5>
                                        <h5 class="font-custom col-md-4"><b>ความสัมพันธ์</b> : @(ViewBag.QueueFullDetail.PayerRelate != null ? ViewBag.QueueFullDetail.PayerRelate : "-")</h5>
                                        <h5 class="font-custom col-md-4"><b>เลขบัตรประชาชน/Passport</b> :@(ViewBag.QueueFullDetail.CardNo != null ? ViewBag.QueueFullDetail.CardNo : "-") </h5>
                                    </div>
                                    <div class="row">
                                        <h5 class="font-custom col-md-4"><b>อาชีพ</b> : @(ViewBag.QueueFullDetail.PayerOccupationLevel != null ? ViewBag.QueueFullDetail.PayerOccupationLevel : "-")</h5>
                                        <h5 class="font-custom col-md-4"><b>เบอร์โทร</b> : @(ViewBag.QueueFullDetail.PayerPhone != null ? ViewBag.QueueFullDetail.PayerPhone : "-")</h5>
                                        <h5 class="font-custom col-md-4"><b>เบอร์โทรที่ทำงาน</b> : @(ViewBag.QueueFullDetail.PayerBuildingPhone != null ? ViewBag.QueueFullDetail.PayerBuildingPhone : "-")</h5>
                                    </div>

                                    <div class="row">
                                        <h5 class="font-custom col-md-4"><b>ชื่อหน่วยงาน</b> : @(ViewBag.QueueFullDetail.PayerBuildingName != null ? ViewBag.QueueFullDetail.PayerBuildingName : "-")</h5>
                                        <h5 class="font-custom col-md-4"><b>การชำระ</b> : @(ViewBag.QueueFullDetail.PayMethodType != null ? ViewBag.QueueFullDetail.PayMethodType : "-")</h5>
                                        <h5 class="font-custom col-md-4"><b>ธนาคาร</b> : @(ViewBag.QueueFullDetail.PayerBankName != null ? ViewBag.QueueFullDetail.PayerBankName : "-")</h5>
                                    </div>
                                    <div class="row">
                                        <h5 class="font-custom col-md-4"><b>เลขบัญชี</b> : @(ViewBag.QueueFullDetail.PayerBankAccountNo != null ? ViewBag.QueueFullDetail.PayerBankAccountNo : "-")</h5>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-md-12" style="text-align:right">
                                            <a href="#" id="btnPayerOther" data-toggle="modal" data-target="#modal-warning" style="text-decoration: underline; color: #2833FF; display: none "></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-sm-12">
                            @if (ViewBag.QueueFullDetail.IsBeware == true)
                            {
                                <div class="box-header" style="background-color: #F12D15; color: #edfcfc; border-radius: 8px 8px; margin-left: 10px; margin-right: 10px ">
                                    <h4 class="font-custom box-title col-md-5 box-title"><i class=" fa fa-exclamation-circle" aria-hidden="true"></i> ข้อมูลเฝ้าระวัง : @(ViewBag.QueueFullDetail.IsBeware != true ? "ไม่มี" : "มี") </h4>
                                    <h4 class="font-custom box-title  col-md-7 box-title">หมายเหตุ : @(ViewBag.QueueFullDetail.BewareRemark != null ? ViewBag.QueueFullDetail.BewareRemark : "-") </h4>
                                </div>
                            }
                            <div class="box-header" style="background-color: #B3EEFF; border-radius: 8px 8px; margin-top: 15px; margin-left: 10px; margin-right: 10px">
                                <h4 class="font-custom box-title col-md-5 ">ผลการตรวจสอบของฝ่าย QC </h4>
                            </div>
                            <form id="QuestionAuditUpdateV2" action="@Url.Action("QuestionAuditUpdateV2")" method="post" enctype="multipart/form-data">
                                <input type="hidden" id="queueAuditId" name="queueAuditId" value="@ViewBag.QueueFullDetail.QueueAuditId" />
                                <div class="box-body shadow  bodyQuestion" style="height: auto;width: auto;margin-top:15px">
                                    <div class="form-group text col-md-12 radio-check status-checked">
                                        <label class="col-md-2 control-label title ">สถานะการตรวจสอบ : </label>
                                        @foreach (var item in ViewBag.QueueAuditStatus)
                                        {
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="queueAuditStatusId" value="@item.QueueAuditStatusId" id="queueAuditStatusId_@item.QueueAuditStatusId" @(item.QueueAuditStatusId == ViewBag.QueueFullDetail.QueueAuditStatusId ? "checked" : "")> @item.AuditStatusDetail
                                            </label>
                                        }
                                    </div>
                                </div>
                                <div id="audit_wait" class="box-body shadow" style="height: auto;width: auto;margin-top:15px ;display:none">
                                    <div class="form-group text col-md-12">
                                        <label class="col-md-2 control-label title">สถานะการโทร : </label>
                                        <div class="col-md-4">
                                            <select class="form-control" id="callStatusId" name="callStatusId" required>
                                                <option value="-1">กรุณาเลือก</option>
                                                @foreach (var item in ViewBag.CallStatusList)
                                                {
                                                    <option value="@item.CallStatusId" @(item.CallStatusId == ViewBag.QueueFullDetail.AuditCallStatusId ? "selected" : "")>@item.CallStatusDetail</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group text col-md-12">
                                        <label class="col-md-2 control-label title">หมายเหตุ : </label>
                                        <div class="col-md-10">
                                            <textarea name="auditRemarkCallStatusId" id="auditRemarkCallStatusId" style="display: block;border-radius: 5px;width: 80%;height: 80px"> @(ViewBag.QueueFullDetail.AuditRemark)</textarea>
                                        </div>
                                    </div>
                                </div>

                                <div id="audit_NotAudit" class="box-body shadow" style="height: auto;width: auto;margin-top:15px ;display:none">
                                    <div class="form-group text col-md-12">
                                        <label class="col-md-2 control-label title">สาเหตุ : </label>
                                        <div class="col-md-4">
                                            <select class="form-control" id="NotAuditedCause" name="NotAuditedCause" required>
                                                <option value="-1">กรุณาเลือก</option>
                                                @foreach (var item in ViewBag.NotAuditedCause)
                                                {
                                                    <option value="@item.NotAuditedCauseId" @(item.NotAuditedCauseId == ViewBag.QueueFullDetail.NotAuditedCauseId ? "selected" : "")>@item.NotAuditedCauseDetail</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group text col-md-12">
                                        <label class="col-md-2 control-label title">หมายเหตุ : </label>
                                        <div class="col-md-10">
                                            <textarea name="auditRemarkNotAuditedCause" id="auditRemarkNotAuditedCause" style="display: block;border-radius: 5px;width: 80%;height: 80px">@(ViewBag.QueueFullDetail.AuditRemark)</textarea>
                                        </div>
                                    </div>
                                </div>
                                <div id="audit_complete" class="box-body shadow bodyQuestionForm" style="height: auto; width: auto; display: none">
                                    @* ----------------1. การนำเสนอขาย และบริการหลังการขาย---------------- *@
                                    <div class="box-header" style="background-color: #F39C12; color:azure; border-radius: 8px 8px">
                                        <h4 class="font-custom box-title col-md-5 ">1. การนำเสนอขาย และบริการหลังการขาย </h4>
                                    </div>
                                    <div class="box-body">
                                        <div class="form-group text col-md-12 radio-check bodyQuestion" style="margin-top:15px">
                                            <label class="col-md-12 control-label title">1.1 วิธีการขาย : </label>
                                            @foreach (var item in ViewBag.SaleMethodType)
                                            {
                                                <label class="col-md-2 fonts-box">
                                                    <input type="radio" name="saleMethodType" value="@item.SaleMethodTypeId" id="saleMethod_@item.SaleMethodTypeId" @(item.SaleMethodTypeId == ViewBag.QueueFullDetail.SaleMethodTypeId ? "checked" : "")> @item.SaleMethodTypeDetail
                                                </label>
                                            }
                                            <label class="saleMethodTypeRemarkLabel col-md-4 fonts-box " id="saleMethodTypeRemarkLabel" style="">
                                                <input class="form-control" id="saleMethodTypeRemark" name="saleMethodTypeRemark" type="text" placeholder="รายละเอียดเพิ่มเติม" value="@(ViewBag.QueueFullDetail.SaleMethodTypeRemark)" />
                                            </label>
                                        </div>
                                        <div class="form-group text col-md-12 radio-check bodyQuestion">
                                            <label class="col-md-12 control-label title">1.2 บริการหลังการขาย : </label>
                                            @foreach (var item in ViewBag.SaleServiceType)
                                            {
                                                <label class="col-md-2 fonts-box">
                                                    <input type="radio" name="saleServiceType" value="@item.SaleServiceTypeId" id="saleServiceType_@item.SaleServiceTypeId" @(item.SaleServiceTypeId == ViewBag.QueueFullDetail.SaleServiceTypeId ? "checked" : "")> @item.SaleServiceTypeDetail

                                                </label>
                                            }

                                            <label class="saleServiceTypeRemarkLabel col-md-4 fonts-box " id="saleServiceTypeRemarkLabel" style="">
                                                <input class="form-control" id="saleServiceTypeRemark" name="saleServiceTypeRemark" type="text" placeholder="รายละเอียดเพิ่มเติม" value="@(ViewBag.QueueFullDetail.SaleServiceTypeRemark)" />
                                            </label>
                                        </div>
                                        <div class="form-group text col-md-12 radio-check bodyQuestion">
                                            <label class="col-md-12 control-label title">
                                                1.3 มอบบัตรให้กับ : @(ViewBag.QueueFullDetail.IsUnderwriteCust == "ผู้เอาประกัน" ? ViewBag.QueueFullDetail.IsUnderwriteCust:"-")/@(ViewBag.QueueFullDetail.IsUnderwritePayer == "ผู้ชำระเบี้ย" ? ViewBag.QueueFullDetail.IsUnderwritePayer : "-")
                                            </label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="cobrandInsuredIsValid" value="1" id="cobrandInsuredIsValid_1" @(ViewBag.QueueFullDetail.CobrandInsuredIsValid == true ? "checked" : "")> ถูกต้อง
                                            </label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="cobrandInsuredIsValid" value="0" id="cobrandInsuredIsValid_2" @(ViewBag.QueueFullDetail.CobrandInsuredIsValid == false ? "checked" : "")> ไม่ถูกต้อง
                                            </label>
                                            <label class="cobrandInsuredRemarkLabel col-md-4 fonts-box " id="cobrandInsuredRemarkLabel" style="">
                                                <input class="form-control" id="cobrandInsuredRemark" name="cobrandInsuredRemark" type="text" placeholder="รายละเอียดเพิ่มเติม" value="@(ViewBag.QueueFullDetail.CobrandInsuredRemark)" />
                                            </label>
                                        </div>
                                        <div class="form-group text col-md-12 radio-check bodyQuestion">
                                            <label class="col-md-12 control-label title">1.4 ประเภทบัตร : <text style="color:black;font-weight:100">@ViewBag.QueueFullDetail.IsLineOADetail </text></label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="lineOAIsValid" value="1" id="lineOAIsValid_1" @(ViewBag.QueueFullDetail.LINEOAIsValid == true ? "checked" : "")> ถูกต้อง
                                            </label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="lineOAIsValid" value="0" id="lineOAIsValid_2" @(ViewBag.QueueFullDetail.LINEOAIsValid == false ? "checked" : "")> ไม่ถูกต้อง
                                            </label>
                                            <label class="lineOARemarkLabel col-md-4 fonts-box bodyQuestion" id="lineOARemarkLabel" style="">
                                                <input class="form-control" id="lineOARemark" name="lineOARemark" type="text" placeholder="รายละเอียดเพิ่มเติม" value="@(ViewBag.QueueFullDetail.LINEOARemark)" />
                                            </label>
                                        </div>
                                        <div class="form-group text col-md-12 radio-check bodyQuestion">
                                            <label class="col-md-12 control-label title">หมายเหตุการมอบบัตร : <text style="color:black;font-weight:100"> - </text></label>
                                        </div>
                                        <div class="form-group text col-md-12 radio-check bodyQuestion">
                                            <label class="col-md-4 control-label title">1.5 ข้อมูล ผอ.บล : <text style="color:black;font-weight:100">@ViewBag.QueueFullDetail.UnderwriteByUserPersonName </text> </label>
                                            <input type="hidden" class="form-control" id="underwriteByUserId" name="underwriteByUserId" value="6772" placeholder="รายละเอียดเพิ่มเติม" />
                                            <label class="underwriteByUserRemarkLabel col-md-4 fonts-box " id="underwriteByUserRemarkLabel" style="">
                                                <input class="form-control" id="underwriteByUserRemark" name="underwriteByUserRemark" type="text" placeholder="รายละเอียดเพิ่มเติม" value="@(ViewBag.QueueFullDetail.UnderwriteByUserRemark)" />
                                            </label>
                                        </div>
                                        <div class="form-group text col-md-12 radio-check bodyQuestion">
                                            <label class="col-md-12 control-label title">1.6 เพิ่มเพื่อนกับสยามสไมล์ทาง line OA :</label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="addLINEIsSuccess" value="1" id="addLINEIsSuccess_1" @(ViewBag.QueueFullDetail.AddLINEIsSuccess == true ? "checked" : "")> เรียบร้อย
                                            </label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="addLINEIsSuccess" value="0" id="addLINEIsSuccess_2" @(ViewBag.QueueFullDetail.AddLINEIsSuccess == false ? "checked" : "")> ไม่เรียบร้อย
                                            </label>
                                        </div>
                                        <div class="form-group text col-md-12 radio-check bodyQuestion">
                                            <label class="col-md-12 control-label title">1.7 รูปแบบบัตรประกัน :</label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="insuranceCardType" value="1" id="insuranceCardType_1" @(ViewBag.QueueFullDetail.InsuranceCardTypeId == 1 ? "checked" : "")> Line OA
                                            </label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="insuranceCardType" value="2" id="insuranceCardType_2" @(ViewBag.QueueFullDetail.InsuranceCardTypeId == 2 ? "checked" : "")> Co-brand
                                            </label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="insuranceCardType" value="3" id="insuranceCardType_2" @(ViewBag.QueueFullDetail.InsuranceCardTypeId == 3 ? "checked" : "")> ไม่ได้รับ
                                            </label>
                                        </div>
                                        <div class="form-group text col-md-12 radio-check bodyQuestion">
                                            <label class="col-md-12 control-label title">1.8 คู่มือ :</label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="isReceivedManual" value="1" id="isReceivedManual_1" @(ViewBag.QueueFullDetail.IsReceivedManual == true ? "checked" : "")> ได้รับ
                                            </label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="isReceivedManual" value="0" id="isReceivedManual_2" @(ViewBag.QueueFullDetail.IsReceivedManual == false ? "checked" : "")> ไม่ได้รับ
                                            </label>
                                            <div class="col-md-4 receivingManual" id="receivingManual" style="display:none">
                                                <select class="form-control" id="receivingManualTypeId" name="receivingManualTypeId">
                                                    <option value="0">กรุณาเลือก</option>
                                                    @foreach (var item in ViewBag.ReceivingManualType)
                                                    {
                                                        <option value="@item.ReceivingManualTypeId" @(item.ReceivingManualTypeId == ViewBag.QueueFullDetail.ReceivingManualTypeId ? "selected" : "")>@item.ReceivingManualTypeDetail</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group text col-md-12 bodyQuestion">
                                            <label class="col-md-12 control-label title">1.9 คะแนนความพึงพอใจ :</label>
                                            <div class="col-md-4">
                                                <select name="feedbackRate" id="feedbackRate" class="form-control">
                                                    <option value="-1" id="0">เลือกคะแนน</option>
                                                    <option value="1" id="1" @(1 == ViewBag.QueueFullDetail.FeedbackRate ? "selected" : "")>1</option>
                                                    <option value="2" id="2" @(2 == ViewBag.QueueFullDetail.FeedbackRate ? "selected" : "")>2</option>
                                                    <option value="3" id="3" @(3 == ViewBag.QueueFullDetail.FeedbackRate ? "selected" : "")>3</option>
                                                    <option value="4" id="4" @(4 == ViewBag.QueueFullDetail.FeedbackRate ? "selected" : "")>4</option>
                                                    <option value="5" id="5" @(5 == ViewBag.QueueFullDetail.FeedbackRate ? "selected" : "")>5</option>
                                                    <option value="6" id="6" @(0 == ViewBag.QueueFullDetail.FeedbackRate ? "selected" : "")>ไม่ประเมิน</option>
                                                </select>
                                            </div>
                                            <label class="feedbackRemarkLabel col-md-4  fonts-box " id="feedbackRemarkLabel" style="">
                                                <input class="form-control" id="feedbackRemark" name="feedbackRemark" type="text" placeholder="รายละเอียดเพิ่มเติม" value="@(ViewBag.QueueFullDetail.FeedbackRemark)" />
                                            </label>
                                        </div>
                                        <div class="form-group text col-md-12 radio-check bodyQuestion">
                                            <label class="col-md-12 control-label title">1.10 ผลด้านบริการ : </label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="serviceResultIsIssue" value="1" id="serviceResultIsIssue_1" @(ViewBag.QueueFullDetail.ServiceResultIsIssue == true ? "checked" : "")> มีประเด็น
                                            </label>
                                            <label class="col-md-2 fonts-box">
                                                <input type="radio" name="serviceResultIsIssue" value="0" id="serviceResultIsIssue_2" @(ViewBag.QueueFullDetail.ServiceResultIsIssue == false ? "checked" : "")> ไม่มีประเด็น
                                            </label>
                                            <label class="serviceResultRemarkLabel col-md-4 fonts-box" id="serviceResultRemarkLabel" style="">
                                                <input class="form-control" id="serviceResultRemark" name="serviceResultRemark" type="text" placeholder="รายละเอียดเพิ่มเติม" value="@(ViewBag.QueueFullDetail.ServiceResultRemark)" />
                                            </label>
                                        </div>
                                    </div>
                                    @* ----------------2. ยืนยันข้อมูลส่วนบุคคล---------------- *@
                                    <div class="box-header" style="background-color: #FFF38F; border-radius: 8px 8px">
