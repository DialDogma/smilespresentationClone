@{
    ViewBag.Title = "MotorAuditIndexV2";
    Layout = "~/Views/Shared/_LayoutForTopPortalIndex.cshtml";
}
<style>
    .page-content img {
        max-width: 20%;
        height: auto !important;
    }

    #overlay {
        position: fixed; /* Sit on top of the page content */
        display: none; /* Hidden by default */
        width: 100%; /* Full width (cover the whole page) */
        height: 100%; /* Full height (cover the whole page) */
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background-color: rgba(0,0,0,0.5); /* Black background with opacity */
        z-index: 2; /* Specify a stack order in case you're using a different order for other elements */
        cursor: not-allowed; /* Add a pointer on hover */
    }

    strong {
        color: red;
    }

    label.title {
        font-family: Sarabun;
        color: #3F8FBD
    }

    label.fonts-box {
        font-family: Sarabun;
        font-size: 14px;
        font-weight: lighter;
    }

    h4.font-custom {
        font-family: Sarabun;
    }

    h5.font-custom {
        font-family: Sarabun;
    }
    .error {
        color: black
    }
</style>
<div class="row">
    <div class="col-xs-12" style="background-color: #1E90FF; color: white; text-align: center">
        <h4>รายละเอียดกรมธรรม์</h4>
    </div>
    <br />
    <br />
    <br />
    <section class="">
        <div class="nav-tabs-custom">
            <ul class="nav nav-tabs">
                <li class="active"><a href="#tab_1" data-toggle="tab">บันทึกผลการทำงาน</a></li>
                <li class=""><a href="#tab_2" data-toggle="tab">ผลการมอบกรมธรรม์</a></li>
                <li class=""><a href="#tab_3" data-toggle="tab">เอกสาร</a></li>
                @if (ViewBag.AccessRole == "AUDIT_DEV" || ViewBag.AccessRole == "AUDIT_BPT" || ViewBag.AccessRole == "AUDIT_INSURE_Motor")
                {
                    <li class=""><a href="#tab_4" data-toggle="tab">ประวัติคิวงาน(ฝ่าย QC)</a></li>
                }
            </ul>
            @if (ViewBag.QueueMotorFullDetail != null)
            {
                <div class="tab-content">
                    @* tab_1 *@
                    <div class="tab-pane active" id="tab_1">
                        @* start ข้อมูลพื้นฐาน *@
                        <div class="row " style="padding:10px">
                            <div class="col-sm-12 ">
                                <div class="box box-solid collapsed-box" style="background-color: #E0F7FF; border-radius: 8px 8px; ">
                                    <div class="box-header" style="color: #3F8FBD; border-bottom: 1px solid #D2D6DE">
                                        <i class="fa fa-newspaper-o"></i>
                                        <h4 class="box-title font-custom">ข้อมูลพื้นฐาน </h4>
                                        <div class="box-tools pull-right">
                                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                        </div>
                                    </div>
                                    <div class="box-body" style="align-items: center; display: block;">
                                        <div class="row">
                                            <h5 class="font-custom col-md-3"><b>App ID</b> : @ViewBag.QueueMotorFullDetail.ApplicationCode</h5>
                                            <h5 class="font-custom col-md-3">
                                                <b>วันเริ่มคุ้มครอง</b>  : @(String.Format("{0:dd MMM yyyy}", ViewBag.QueueMotorFullDetail.StartCoverDate))
                                            </h5>
                                            <h5 class="font-custom col-md-3"><b>แผนประกัน</b> : @ViewBag.QueueMotorFullDetail.ProductDetail</h5>
                                            <h5 class="font-custom col-md-3"><b>เบี้ย</b> : @ViewBag.QueueMotorFullDetail.Premium บาท</h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-3 "><b>บริษัทประกัน</b> : @ViewBag.QueueMotorFullDetail.InsuranceCompanyDetail</h5>
                                            <h5 class="font-custom col-md-3"><b>ผู้แทน</b> : @ViewBag.QueueMotorFullDetail.SaleEmployeeCode @ViewBag.QueueMotorFullDetail.SaleEmployeeName</h5>
                                            <h5 class="font-custom col-md-3"><b>รหัสเจ้าของรถ</b> : @ViewBag.QueueMotorFullDetail.ZebraCarOwnerEmployeeCode</h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12" style="margin-top:15px;">
                                <div class="box box-solid collapsed-box" style="background-color: #E0F7FF; border-radius: 8px 8px">
                                    <div class="box-header" style="color: #3F8FBD; border-bottom: 1px solid #bbb">
                                        <i class="fa fa-car"></i>
                                        <h4 class="box-title font-custom">รายละเอียดรถยนต์ </h4>
                                        <div class="box-tools pull-right">
                                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                        </div>
                                    </div>
                                    <div class="box-body" style="align-items: center;">
                                        <div class="row">

                                            <h5 class="font-custom col-md-4"><b>ยี่ห้อ</b> : @(String.IsNullOrEmpty(ViewBag.QueueMotorFullDetail.VehicleBrandDetail) ? "-" : ViewBag.QueueMotorFullDetail.VehicleBrandDetail)</h5>
                                            <h5 class="font-custom col-md-4"><b>รุ่น</b> : @(String.IsNullOrEmpty(ViewBag.QueueMotorFullDetail.VehicleModelDetail) ? "-" : ViewBag.QueueMotorFullDetail.VehicleModelDetail)</h5>
                                            <h5 class="font-custom col-md-4"><b>เลขทะเบียน</b> :@(String.IsNullOrEmpty(ViewBag.QueueMotorFullDetail.VehicleRegistrationNumber) ? "-" : ViewBag.QueueMotorFullDetail.VehicleRegistrationNumber)</h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-4"><b>ปีที่จดทะเบียน</b> : @ViewBag.QueueMotorFullDetail.VehicleRegistrationYear</h5>
                                            <h5 class="font-custom col-md-4"><b>หมายเลขตัวถัง</b> :@(String.IsNullOrEmpty(ViewBag.QueueMotorFullDetail.VehicleChassisNumber) ? "-" : ViewBag.QueueMotorFullDetail.VehicleChassisNumber)</h5>
                                            <h5 class="font-custom col-md-4"><b>ประเภทยานพาหนะ</b> :@(String.IsNullOrEmpty(ViewBag.QueueMotorFullDetail.VehicleTypeDetail) ? "-" : ViewBag.QueueMotorFullDetail.VehicleTypeDetail)</h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-4"><b>ประเภทการใช้งาน</b> :@(String.IsNullOrEmpty(ViewBag.QueueMotorFullDetail.VehicleUseTypeDetail) ? "-" : ViewBag.QueueMotorFullDetail.VehicleUseTypeDetail)</h5>
                                            <h5 class="font-custom col-md-4"><b>ลักษณะการใช้งาน</b> :@(String.IsNullOrEmpty(ViewBag.QueueMotorFullDetail.DriveDetail) ? "-" : ViewBag.QueueMotorFullDetail.DriveDetail)</h5>
                                            <h5 class="font-custom col-md-4"><b>ขนาดเครื่องยนต์ (L)</b> :@(String.IsNullOrEmpty(ViewBag.QueueMotorFullDetail.VehicleEngineSize) ? "-" : ViewBag.QueueMotorFullDetail.VehicleEngineSize)</h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-4"><b>ทุนประกันรถยนต์</b> : @ViewBag.QueueMotorFullDetail.SumInsured</h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12" style="margin-top:15px;">
                                <div class="box box-solid collapsed-box" style="background-color: #E0F7FF; border-radius: 8px 8px">
                                    <div class="box-header" style="color: #3F8FBD; border-bottom: 1px solid #bbb">
                                        <i class="fa fa-user"></i>
                                        <h4 class="box-title font-custom">ข้อมูลผู้เอาประกัน </h4>
                                        <div class="box-tools pull-right">
                                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                        </div>
                                    </div>
                                    <div class="box-body">
                                        <div class="row">
                                            <h5 class="font-custom col-md-12 "><b>ประเภทบัตร/หมายเลขบัตร</b> : -</h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-4 ">
                                                <b>ประเภท</b> : @(ViewBag.QueueMotorFullDetail.InsuredTypeDetail!= null ?ViewBag.QueueMotorFullDetail.InsuredTypeDetail :"-")
                                            </h5>
                                            <h5 class="font-custom col-md-4"><b>ชื่อ</b> :@(ViewBag.QueueMotorFullDetail.InsuredName != null ? ViewBag.QueueMotorFullDetail.InsuredName :"-")</h5>
                                            <h5 class="font-custom col-md-4"><b>วันเกิด</b> : @(ViewBag.QueueMotorFullDetail.InsuredBirthDate != null ? String.Format("{0:dd MMM yyyy}", ViewBag.QueueMotorFullDetail.InsuredBirthDate) : "-")</h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-4 "><b>โทรศัพท์มือถือ</b> :@(ViewBag.QueueMotorFullDetail.InsuredPhone != null ? ViewBag.QueueMotorFullDetail.InsuredPhone:"-")</h5>
                                            <h5 class="font-custom col-md-4"><b>กลุ่มอาชีพ</b> :  @(ViewBag.QueueMotorFullDetail.InsuredOccupation != null ? ViewBag.QueueMotorFullDetail.InsuredOccupation:"-")</h5>
                                            <h5 class="font-custom col-md-4"><b>อาชีพ</b> : @(ViewBag.QueueMotorFullDetail.InsuredOccupationLevel != null ? ViewBag.QueueMotorFullDetail.InsuredOccupationLevel : "-") </h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-4 "><b>ชื่อหน่วยงาน</b> : @(ViewBag.QueueMotorFullDetail.InsuredOfficeName != null ?ViewBag.QueueMotorFullDetail.InsuredOfficeName : "-")</h5>
                                            <h5 class="font-custom col-md-4"><b>ลักษณะงาน</b> : @(ViewBag.QueueMotorFullDetail.InsuredOccupationDetail != null ? ViewBag.QueueMotorFullDetail.InsuredOccupationDetail :"-")</h5>
                                            <h5 class="font-custom col-md-4"><b>ที่อยู่</b> : @(ViewBag.QueueMotorFullDetail.InsuredAddress!= null ? ViewBag.QueueMotorFullDetail.InsuredAddress :"-")</h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12" style="margin-top:15px;">
                                <div class="box box-solid collapsed-box" style="background-color: #E0F7FF; border-radius: 8px 8px; ">
                                    <div class="box-header" style="color: #3F8FBD; border-bottom: 1px solid #bbb">
                                        <i class="fa fa-user"></i>
                                        <h4 class="box-title">ข้อมูลผู้ชำระเบี้ย </h4>
                                        <div class="box-tools pull-right">
                                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                        </div>
                                    </div>
                                    <div class="box-body">
                                        <div class="row">
                                            <h5 class="font-custom col-md-12 "><b>ประเภทบัตร/หมายเลขบัตร</b> : @(ViewBag.QueueMotorFullDetail.PayerIdentityCard != null ? ViewBag.QueueMotorFullDetail.PayerIdentityCard :"-")</h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-4 "><b>ประเภท</b> : -</h5>
                                            <h5 class=" font-custom col-md-4"><b>ชื่อ</b> :@(ViewBag.QueueMotorFullDetail.PayerName != null ? ViewBag.QueueMotorFullDetail.PayerName:"-")</h5>
                                            <h5 class="font-custom col-md-4">
                                                <b>วันเกิด</b> :@(ViewBag.QueueMotorFullDetail.PayerBirthDate != null ? String.Format("{0:dd MMM yyyy}", ViewBag.QueueMotorFullDetail.PayerBirthDate) : "-")
                                            </h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-4 "><b>โทรศัพท์มือถือ</b> :@(ViewBag.QueueMotorFullDetail.PayerPhone != null ? ViewBag.QueueMotorFullDetail.PayerPhone :"-")</h5>
                                            <h5 class="font-custom col-md-4"><b>กลุ่มอาชีพ</b> :  @(ViewBag.QueueMotorFullDetail.PayerOccupation!= null ? ViewBag.QueueMotorFullDetail.PayerOccupation :"-")</h5>
                                            <h5 class="font-custom col-md-4"><b>อาชีพ</b> : @(ViewBag.QueueMotorFullDetail.PayerOccupationLevel != null ? ViewBag.QueueMotorFullDetail.PayerOccupationLevel:"-")</h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom  col-md-4 "><b>ชื่อหน่วยงาน</b> : @(ViewBag.QueueMotorFullDetail.PayerOfficeName != null ? ViewBag.QueueMotorFullDetail.PayerOfficeName :"-")</h5>
                                            <h5 class="font-custom col-md-4"><b>ลักษณะงาน</b> : @(ViewBag.QueueMotorFullDetail.PayerOccupationDetail != null ? ViewBag.QueueMotorFullDetail.PayerOccupationDetail :"-")</h5>
                                            <h5 class="font-custom col-md-4"><b>ที่อยู่</b> : @(ViewBag.QueueMotorFullDetail.PayerAddress!= null ? ViewBag.QueueMotorFullDetail.PayerAddress :"-")</h5>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-sm-12" style="margin-top:15px; margin-bottom:15px">
                                <div class="box box-solid collapsed-box" style="background-color: #E0F7FF; border-radius: 8px 8px; ">
                                    <div class="box-header" style="color: #3F8FBD; border-bottom: 1px solid #bbb">
                                        <i class="fa fa-user"></i>
                                        <h4 class="box-title font-custom">ข้อมูลผู้ใช้รถ </h4>
                                        <div class="box-tools pull-right">
                                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                        </div>
                                    </div>
                                    <div class="box-body">
                                        <div class="row">
                                            <h5 class="font-custom col-md-4 "><b>ชื่อ</b> : @(ViewBag.QueueMotorFullDetail.DriverName != null ? ViewBag.QueueMotorFullDetail.DriverName : "-") </h5>
                                            <h5 class="font-custom col-md-4"><b>วันเกิด</b> :  <span class="control-label" display: inline-block; max-width: 100%; text-align: right">@(ViewBag.QueueMotorFullDetail.DriverBirthDate != null ? String.Format("{0:dd MMM yyyy}", ViewBag.QueueMotorFullDetail.DriverBirthDate) : "-")</span></h5>
                                            <h5 class="font-custom col-md-4"><b>เบอร์โทร</b> : @(ViewBag.QueueMotorFullDetail.DriverPhone!= null ? ViewBag.QueueMotorFullDetail.DriverPhone : "-")</h5>
                                        </div>
                                        <div class="row">
                                            <h5 class="font-custom col-md-4"><b>กลุ่มอาชีพ</b> :  @(ViewBag.QueueMotorFullDetail.DriverOccupation != null ? ViewBag.QueueMotorFullDetail.DriverOccupation :  "-")</h5>
                                            <h5 class="font-custom col-md-4"><b>อาชีพ</b> : @(ViewBag.QueueMotorFullDetail.DriverOccupationLevel != null ? ViewBag.QueueMotorFullDetail.DriverOccupationLevel :  "-")</h5>
                                            <h5 class="font-custom col-md-4"><b>ลักษณะงาน</b> : @(ViewBag.QueueMotorFullDetail.DriverOccupationDetail!= null ? ViewBag.QueueMotorFullDetail.DriverOccupationDetail : "-")</h5>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row ">
                            <div class="col-sm-12">

                                @if (ViewBag.QueueMotorFullDetail.IsBeware == true)
                    {
                                    <div class="box-header" style="background-color: #F12D15; color: #edfcfc; border-radius: 8px 8px; margin-left: 10px; margin-right: 10px ">
                                        <h4 class="font-custom col-md-5 box-title "><i class=" fa fa-exclamation-circle" aria-hidden="true"></i> ข้อมูลเผ้าระวัง : @(ViewBag.QueueMotorFullDetail.IsBeware != true ? "ไม่มี" : "มี") </h4>
                                        <h4 class="font-custom col-md-7 box-title">หมายเหตุ : @(ViewBag.QueueMotorFullDetail.BewareRemark != null ? ViewBag.QueueMotorFullDetail.BewareRemark : "-") </h4>
                                    </div>
                                }
                                <div class="box-header" style="background-color: #B3EEFF; border-radius: 8px 8px; margin-top: 15px; margin-left: 10px; margin-right: 10px ">
                                    <h4 class="font-custom box-title col-md-5 ">ผลการตรวจสอบของฝ่าย QC </h4>
                                </div>
                                <form id="QuestionAuditUpdateV2" class="bodyQuestion" action="@Url.Action("QuestionAuditUpdateV2")" method="post" enctype="multipart/form-data">
                                    <input type="hidden" id="queueAuditId" name="queueAuditId" value="@ViewBag.QueueMotorFullDetail.QueueAuditId" />
                                    <div class="box-body shadow  bodyQuestion" style="height: auto;width: auto;margin-top:15px">
                                        <div class="form-group text col-md-12 radio-check status-checked bodyQuestion">
                                            <label class="col-md-2 control-label title ">สถานะการตรวจสอบ : </label>

                                            @foreach (var item in ViewBag.QueueAuditStatus)
                {
                                                <label class="col-md-2 fonts-box">
                                                    <input type="radio" name="queueAuditStatusId" value="@item.QueueAuditStatusId" id="queueAuditStatusId_@item.QueueAuditStatusId" @(ViewBag.QueueMotorFullDetail.QueueAuditStatusId == item.QueueAuditStatusId ? "checked" : "")> @item.QueueAuditStatusDetail
                                                </label>
                                                <input type="hidden" name="queueAuditStatusId_checked" value="@ViewBag.QueueMotorFullDetail.QueueAuditStatusId" id="queueAuditStatusId_checked">

                                            }
                                        </div>
                                    </div>
                                    <div id="audit_wait" class="box-body shadow audit_wait" style="height: auto; width: auto; margin-top: 15px; display: none">
                                        <div class="form-group text col-md-12 ">
                                            <label class="col-md-2 control-label title">สถานะการโทร : </label>
                                            <div class="col-md-4 ">
                                                <select class="form-control error" id="callStatusId" name="callStatusId" required>
                                                    <option value="-1">กรุณาเลือก</option>
                                                    @if (ViewBag.CallStatusListAuditMotor != null)
                                                    {
                foreach (var item in ViewBag.CallStatusListAuditMotor)
                {
                                                            <option value="@item.CallStatusId" @(ViewBag.QueueMotorFullDetail.CallStatusId == item.CallStatusId ? "selected" : "")>@item.CallStatusDetail</option>
                                                        }
            }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group text col-md-12">
                                            <label class="col-md-2 control-label title">หมายเหตุ : </label>
                                            <div class="col-md-10 ">
                                                <textarea name="auditRemarkCallStatusId" id="auditRemarkCallStatusId" style="display: block;border-radius: 5px;width: 80%;height: 80px">@(ViewBag.QueueMotorFullDetail.AuditRemark)</textarea>
                                            </div>
                                        </div>
                                    </div>

                                    <div id="audit_NotAudit" class="box-body shadow audit_NotAudit" style="height: auto;width: auto;margin-top:15px ;display:none">
                                        <div class="form-group text col-md-12 ">
                                            <label class="col-md-2 control-label title">สาเหตุ : </label>
                                            <div class="col-md-4 ">
                                                <select class="form-control error" id="notAuditedCause" name="notAuditedCause" required>
                                                    <option value="-1">กรุณาเลือก</option>
                                                    @foreach (var item in ViewBag.NotAuditedCause)
                                                    {
                                                        <option value="@item.NotAuditedCauseId" @(ViewBag.QueueMotorFullDetail.NotAuditedCauseId == item.NotAuditedCauseId ? "selected" : "")>@item.NotAuditedCauseDetail</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group text col-md-12">
                                            <label class="col-md-2 control-label title">หมายเหตุ : </label>
                                            <div class="col-md-10 ">
                                                <textarea name="auditRemarkNotAuditedCause" id="auditRemarkNotAuditedCause" style="display: block;border-radius: 5px;width: 80%;height: 80px">@(ViewBag.QueueMotorFullDetail.AuditRemark)</textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="audit_complete" class="box-body shadow bodyQuestionForm" style="height: auto; width: auto;display: none ">
                                        <div class="box-body">
                                            <div class="form-group text col-md-12 radio-check bodyQuestion">
                                                <label class="col-md-12 control-label title">ผู้ให้ข้อมูล : </label>
                                                <label class="col-md-2 fonts-box">
                                                    <input type="checkbox" name="IsUnderwriteInsured" id="IsUnderwriteInsured" @(ViewBag.QueueMotorFullDetail.IsUnderwriteInsured == true ? "checked" : "")> ผู้เอาประกัน
                                                </label>
                                                <label class="col-md-2 fonts-box">
                                                    <input type="checkbox" name="IsUnderwritePayer" id="IsUnderwritePayer" @(ViewBag.QueueMotorFullDetail.IsUnderwritePayer == true ? "checked" : "")> ผู้ชำระเบี้ย
                                                </label>
                                            </div>
                                            <div class="form-group text col-md-12 radio-check bodyQuestion">
                                                <label class="col-md-12 control-label title">เป็นบุคคลเดียวกับผู้ชำระเบี้ยหรือไม่ :</label>
                                                <label class="col-md-2 fonts-box">
                                                    <input type="radio" name="IsSamePersonPayer" id="IsSamePersonPayer_1" value="1" @(ViewBag.QueueMotorFullDetail.IsSamePersonPayer == true ? "checked" : "")> ใช่
                                                </label>
                                                <label class="col-md-2 fonts-box">
                                                    <input type="radio" name="IsSamePersonPayer" id="IsSamePersonPayer_2" value="0" @(ViewBag.QueueMotorFullDetail.IsSamePersonPayer == false ? "checked" : "")> ไม่ใช่
                                                </label>
                                                <label class="IsSamePersonPayerRemarkLabel col-md-4 fonts-box " id="IsSamePersonPayerRemarkLabel" style="">
                                                    <input class="form-control" id="IsSamePersonPayerRemark" name="IsSamePersonPayerRemark" type="text" placeholder="รายละเอียดเพิ่มเติม" value="@(ViewBag.QueueMotorFullDetail.IsSamePersonPayerRemark)" />
                                                </label>
                                            </div>
                                        </div>


                                        @* ----------------1. การนำเสนอขาย และบริการหลังการขาย---------------- *@
                                        <div class="box-header" style="background-color: #B3EEFF; border-radius: 8px 8px; margin-top:15px ">
                                            <h4 class="font-custom box-title col-md-5 ">1. การนำเสนอขาย และบริการหลังการขาย </h4>
                                        </div>
                                        <div class="box-body">
                                            <div class="form-group text col-md-12 radio-check bodyQuestion" style="margin-top:15px">
                                                <label class="col-md-12 control-label title">1.1 วิธีการขาย : </label>
