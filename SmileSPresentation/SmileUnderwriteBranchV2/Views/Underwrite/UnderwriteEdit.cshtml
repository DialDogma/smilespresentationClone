                    $('#showImg').hide();
                    swal.close();
                    Dropzone.forElement('#myDropzone').removeAllFiles(true);
                    $('#editDropzone').show();
                    countFile = 0;

                });
            });


            //FORM SUBMIT
            $('#btnSubmit').click(function () {
                if (ValidateSave()) {
                    if ($('#sectionGiveCardFrom').valid() && $('#sectionGiveCardFrom_2').valid()) {
                        if (countFile == 0) {
                            imgUrlPath = null;
                            imgPhysicalPath = null;
                            fileImgName = null;
                            swal_confirm("ยืนยันบันทึกข้อมูล", "", function (e) {
