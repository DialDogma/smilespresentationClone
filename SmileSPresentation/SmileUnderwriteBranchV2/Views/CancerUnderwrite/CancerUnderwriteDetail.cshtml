@{
    ViewBag.Title = "คิวงานคัดกรองมอบเอกสาร";
    Layout = "~/Views/Shared/_LayoutForTopIndexCancer.cshtml";
}

<style>
    strong {
        color: red;
    }
 
    .btn-warningg:hover {
        box-shadow: rgb(0 0 0 / 20%) 0px 10px 7px -5px, rgb(0 0 0 / 14%) 0px 2px 2px 0px, rgb(0 0 0 / 12%) 0px 1px 5px 0px;
        border-radius: 7px !important;
    }

    .btn-success, .btn-primary, .btn-info, .btn-warning, .btn-danger, .btn-default {
        border: none !important;
    }

    .marker {
        list-style-type: circle;
        color: #005484
    }

    th.sorting_asc::after {
        content: "" !important;
    }
</style>
<div class="row" style="text-align: center; background-color: #ECF0F5; color: #005484">
