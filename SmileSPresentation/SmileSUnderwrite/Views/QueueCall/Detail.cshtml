                        <label>
                            <input type="checkbox" id="chk_option3" name="chk_option3"
                                   @if (ViewBag.ResultQ2 != null && ViewBag.ResultQ2.A6) { <text> checked="checked" </text>  } else if (ViewBag.ResultQ3 != null && ViewBag.ResultQ3.A16) { <text> checked="checked" </text> }>
