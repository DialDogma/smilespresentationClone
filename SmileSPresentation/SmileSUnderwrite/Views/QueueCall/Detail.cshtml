                                   @if (ViewBag.ResultQ2 != null && ViewBag.ResultQ2.A4) { <text> checked="checked" </text>  } else if (ViewBag.ResultQ3 != null && ViewBag.ResultQ3.A14) { <text> checked="checked" </text> }>
