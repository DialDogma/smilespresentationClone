<div class="row">
    @{
        Html.RenderPartial("_ApplicationDetail");
    }
    <div id="UnderwriteHistory_form" class="col-md-12">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h4 class="box-title">ประวัติคัดกรอง</h4>
