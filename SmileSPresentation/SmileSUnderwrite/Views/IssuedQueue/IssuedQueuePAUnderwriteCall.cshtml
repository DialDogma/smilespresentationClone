    @* ผลการโทรคัดกรอง *@
    <div id="PACallResult_form" class="col-md-12">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h4 class="box-title">ผลการโทรคัดกรอง</h4>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                        <i class="fa fa-minus"></i>
                    </button>
                </div>
            </div>
            <div class="box-body">
                <div class="form-group row">
                    <div class="radio col-sm-offset-2">
                        <label>
                            <input type="radio" id="radio_callResult1" name="radio_callResult" value="option1"
                                   @if (ViewBag.ResultQ2 != null && ViewBag.ResultQ2.UnderwriteStatusId == 3) { <text> checked="checked" </text>  } else if (ViewBag.ResultQ3 != null && ViewBag.ResultQ3.UnderwriteStatusId == 3) { <text> checked="checked" </text>  } else if (ViewBag.ResultQ4 != null && ViewBag.ResultQ4.UnderwriteStatusId == 3) { <text> checked="checked" </text> }>
