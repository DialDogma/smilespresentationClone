                    </div>
                </div>
                <div class="form-group row">
                    <div class="checkbox col-sm-offset-3">
                        <label>
                            <input type="checkbox" id="chk_option2_2_4" name="chk_option2_2_1" value="A28"
                                   @if (ViewBag.ResultQ4 != null && ViewBag.ResultQ4.A28) { <text> checked="checked" </text> }>
