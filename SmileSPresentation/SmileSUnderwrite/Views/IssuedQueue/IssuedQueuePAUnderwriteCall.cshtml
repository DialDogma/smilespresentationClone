                    </div>
                </div>
                <div class="form-group row">
                    <div class="radio col-sm-offset-2">
                        <label>
                            <input type="radio" id="radio_callResult2" name="radio_callResult" value="option2"
                                   @if (ViewBag.ResultQ2 != null && ViewBag.ResultQ2.UnderwriteStatusId == 2) { <text> checked="checked" </text>  } else if (ViewBag.ResultQ3 != null && ViewBag.ResultQ3.UnderwriteStatusId == 2) { <text> checked="checked" </text>  } else if (ViewBag.ResultQ4 != null && ViewBag.ResultQ4.UnderwriteStatusId == 2) { <text> checked="checked" </text> }>
                            ปกติ
                        </label>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @* หมายเหตุ *@
    <form id="RemarkMain_Form" action="@Url.Action("IssuedQueuePAUnderwriteCallPost")" method="post">
        <div id="Remark_Form" class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h4 class="box-title">หมายเหตุ</h4>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body">
                    <div class="form-group row">
                        <div class="col-sm-12">
                            <textarea id="txtRemark" name="txtRemark" class="form-control" rows="3">
                                    @if(ViewBag.ResultQ2 != null)
                                    {
                                        @ViewBag.ResultQ2.Remark
                                    }else if(ViewBag.ResultQ3 != null)
                                    {
                                        @ViewBag.ResultQ3.Remark
                                    }else if(ViewBag.ResultQ4 != null)
                                    {
                                        @ViewBag.ResultQ4.Remark
                                    }</textarea>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @* ผลการตรวจสอบ *@
        <div id="CheckResult_form" class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h4 class="box-title">ผลการตรวจสอบ</h4>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body">
                    <div class="form-group row">
                        <div class="col-md-offset-1 col-md-2 checkbox">
                            <label>
                                <input type="checkbox" id="chk_IsCheat" name="chk_IsCheat">
                                ทุจริต
                            </label>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-md-offset-1 col-md-10">
                            <label class="control-label" for="">หมายเหตุ</label>
                            <textarea id="txtRemarkCheck" name="txtRemarkCheck" class="form-control" rows="3"></textarea>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @* อัพโหลดไฟล์ *@
        <div id="Uploadfile_form" class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h4 class="box-title">อัพโหลดไฟล์</h4>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body">
                    <div class="form-group row">
                        <div class="col-sm-offset-1 col-sm-2">
                            <a class="btn btn-block btn-warning" href="@ViewBag.ScanUrl" target="_blank" id="btnScanDoc">ScanDoc</a>
                        </div>
                        <div class=" col-sm-2">
                            <a class="btn btn-block btn-info" href="@ViewBag.OpenUrl" target="_blank" id="btnSeeDoc">จำนวนเอกสาร <span id="docCount">@ViewBag.DocCount</span></a>
