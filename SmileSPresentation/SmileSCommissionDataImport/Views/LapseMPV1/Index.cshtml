                                                <td>@item.SaleTeam_id2</td>
                                                <td>@item.CreatedBy_id</td>
                                                <td>@(item.CreatedDate != null ? item.CreatedDate.ToString("dd/MM/yyyy") : "")</td>
                                                <td>@item.ProductGroup_id</td>
                                                <td>@item.ProductGroup</td>
                                                <td>@item.CancelCause</td>
                                                <td>@(item.PortfolioPeriod != null ? item.PortfolioPeriod.ToString("dd/MM/yyyy") : "")</td>
